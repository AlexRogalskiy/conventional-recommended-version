{
  "name": "conventional-recommended-version",
  "description": "Determine the semantic version number of your project",
  "version": "0.4.0",
  "author": "Jamie Mason <jamie@foldleft.io> (https://github.com/JamieMason)",
  "bin": {
    "conventional-recommended-version": "dist/bin.js"
  },
  "bugs": "https://github.com/JamieMason/conventional-recommended-version/issues",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "commander": "2.11.0",
    "logservable": "0.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "cz-conventional-changelog": "2.0.0",
    "eslint-config-xo": "0.18.2",
    "eslint-plugin-unicorn": "2.1.2",
    "fast-async": "6.3.0",
    "rimraf": "2.6.1",
    "xo": "0.18.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/conventional-recommended-version#readme",
  "keywords": [
    "changelog",
    "commitizen",
    "conventional",
    "conventional-changelog",
    "git",
    "git-log",
    "log"
  ],
  "license": "MIT",
  "main": "conventional-recommended-version",
  "repository": "JamieMason/conventional-recommended-version",
  "scripts": {
    "build": "babel --out-dir ./dist --ignore node_modules ./src",
    "lint": "xo",
    "prebuild": "rimraf dist",
    "prepack": "npm run build",
    "test": "npm run lint"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": true,
    "rules": {
      "import/extensions": 0,
      "import/no-unassigned-import": 0,
      "object-curly-spacing": 0,
      "unicorn/no-process-exit": 0
    },
    "space": 2
  }
}
